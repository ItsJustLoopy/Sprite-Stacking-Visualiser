<Window x:Class="Sprite_Stacking_Visualiser.MainWindow"  
       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"  
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
       xmlns:local="clr-namespace:Sprite_Stacking_Visualiser"  
       xmlns:skia="clr-namespace:SkiaSharp.Views.WPF;assembly=SkiaSharp.Views.WPF"  
       mc:Ignorable="d"  
       Title="MainWindow" Height="450" Width="800">
    <Border BorderBrush="White" BorderThickness="5" Margin="50,50,50,50">
        <Grid Margin="-37,-27,-37,-27">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="305*"/>
                <ColumnDefinition Width="77*"/>
            </Grid.ColumnDefinitions>
            <skia:SKElement x:Name="SkiaCanvas" PaintSurface="SkiaCanvas_PaintSurface" Grid.ColumnSpan="2" Margin="142,78,142,78" />
        </Grid>
    </Border>
</Window>
